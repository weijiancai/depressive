{
    "name": "depressive",
    "displayName": "depressive",
    "description": "depressive",
    "version": "0.0.1",
    "publisher": "wei_jc",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "images/icon.png",
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.browser",
                "title": "open browser"
            },
            {
                "command": "NewRepertoryFile",
                "title": "新建",
                "icon":{
                    "light": "resources/light/AddFile.svg",
					"dark": "resources/dark/AddFile.svg"
                }
            }
        ],
        "configuration": {
            "title": "repertory configuration",
            "properties": {
                "host": {
                    "type": "string",
                    "default": "http://localhost:8888/repertory",
                    "description": "同步主机地址"
                }
            }
        },
        "views": {
            "explorer": [
                {
                    "id": "repertory",
                    "name": "资料库"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "NewRepertoryFile",
                    "when": "view == repertory",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "request": "^2.81.0"
    }
}